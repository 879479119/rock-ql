{
  "name": "rock-builder",
  "version": "0.3.9",
  "private": false,
  "main": "dist/index.js",
  "module": "es/Framework/index.js",
  "lib": "lib/index.js",
  "files": [
    "dist",
    "es"
  ],
  "dependencies": {
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "antd": "^3.13.6",
    "react": "^16.8.3",
    "react-dnd": "^7.0.2",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5",
    "rock-generator": "^0.3.0",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run copycss && npm run build:ts",
    "build:lib": "rollup -c rollup.config.js",
    "build:es": "rollup -c rollup.es.config.js",
    "build:ts": "tsc src/**/**.tsx --jsx react --outDir es --module ESNext --t ES5 --importHelpers",
    "copycss": "rsync -r --include '*/' --include '*.css' --exclude '*' ./src/ ./es/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "rollup": "^1.3.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.0"
  }
}
